{
  "name": "Recorded Future IOC",
  "id": "RecordedFutureIOCPlugin",
  "version": "1.0.0-beta",
  "description": "This plugin is used to pull IOCs of type URL, Domain, IP and Hash from Recorded Future.",
  "patch_supported": false,
  "push_supported": false,
  "configuration": [
    {
      "label": "API Key",
      "key": "apikey",
      "type": "password",
      "mandatory": true,
      "default": "",
      "description": "Input the Recorded Future's ConnectAPI Key."
    },
    {
      "label": "Risk List",
      "key": "risklist",
      "type": "multichoice",
      "choices": [
        {
          "key": "domain",
          "value": "domain"
        },
        {
          "key": "hash",
          "value": "hash"
        },
        {
          "key": "ip",
          "value": "ip"
        },
        {
          "key": "url",
          "value": "url"
        }
      ],
      "mandatory": true,
      "default": [
        "domain",
        "hash",
        "ip",
        "url"
        ],
      "description": "Select the Risk List to import."
    }
  ]
}
